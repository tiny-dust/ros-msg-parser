{
  "name": "ros-msg-parser",
  "version": "0.0.1",
  "description": "ROS消息/服务/动作文件处理脚本集合",
  "type": "module",
  "sideEffects": false,
  "scripts": {
    "dev": "vite build --watch",
    "build": "vite build",
    "clean": "rimraf node_modules dist"
  },
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "ros-to-ts": "dist/ros-to-ts/cli.js"
  },
  "dependencies": {
    "commander": "14.0.0",
    "unconfig": "7.3.3",
    "simple-git": "3.28.0"
  },
  "devDependencies": {
    "@types/glob": "^9.0.0",
    "@types/node": "^24.3.0",
    "ts-node": "^10.9.2",
    "typescript": "~5.9.2",
    "vite-plugin-dts": "^4.3.0"
  }
}
